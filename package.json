{
  "name": "createlen",
  "version": "0.1.0",
  "description": "Landing page generator service (server + export) for legal landing pages",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env MOCK_OPENAI=true nodemon --watch server.js --exec \"node server.js\"",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .js",
    "audit": "bash audit.sh || true"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.4.3"
  }
}