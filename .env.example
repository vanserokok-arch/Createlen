# Copy to .env and fill values. Do NOT commit real secrets.

# OpenAI Configuration (use either OPENAI_KEY or OPENAI_API_KEY)
OPENAI_API_KEY=
OPENAI_KEY=
OPENAI_MODEL=gpt-4o-mini

# API Authentication
ALLOWED_TOKEN=

# Database (Supabase Postgres) - Required for async mode
DATABASE_URL=

# Redis Queue (Upstash) - Required for async mode
REDIS_URL=

# AWS S3 Storage - Required for async mode
S3_BUCKET=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=us-east-1

# Render API (optional)
RENDER_API_KEY=
RENDER_SERVICE_ID=

