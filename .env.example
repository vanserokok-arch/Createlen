# Copy to .env and fill values. Do NOT commit real secrets.
# OpenAI Configuration (supports both OPENAI_KEY and OPENAI_API_KEY)
OPENAI_KEY=
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Authentication
ALLOWED_TOKEN=

# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://user:pass@host:5432/db

# Redis (Upstash for BullMQ)
REDIS_URL=rediss://default:pass@host:port

# S3 Storage (AWS or compatible)
S3_BUCKET=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=us-east-1

# Render Deployment (optional, for CI/CD)
RENDER_API_KEY=
RENDER_SERVICE_ID=
