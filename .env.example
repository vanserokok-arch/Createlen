# Copy to .env and fill values. Do NOT commit real secrets.

# OpenAI Configuration (both keys supported for compatibility)
OPENAI_KEY=
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# API Security
ALLOWED_TOKEN=

# Database (PostgreSQL) - for async mode
DATABASE_URL=

# Redis Queue - for async mode (supports rediss:// for TLS)
REDIS_URL=

# S3 Storage - for async mode
S3_BUCKET=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=us-east-1

# Worker Configuration
WORKER_CONCURRENCY=2

# Render API (optional)
RENDER_API_KEY=
RENDER_SERVICE_ID=
